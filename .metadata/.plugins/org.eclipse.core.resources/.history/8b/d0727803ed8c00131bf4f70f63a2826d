
/*
 * This is the Account class containing info about players and administrators
 * @author Mehrdad Dehdashti
 */
public class Account
{
	private String aFirstName;
	private String aLastName;
	private int aAge;
	private String aUserName;
	private String aPassword;
	private String aIPAddress;
	private boolean bOnline;
	
	public Account(String pFirstName, String pLastName, int aAge, )
	
	public String getFirstName() {
		return aFirstName;
	}
	
	public void setFirstName(String pFirstName) {
		aFirstName = pFirstName;
	}
	
	public String getLastName() {
		return aLastName;
	}
	
	public void setLastName(String pLastName) {
		aLastName = pLastName;
	}

	public int getAge() {
		return aAge;
	}

	public void setAge(int aAge) {
		this.aAge = aAge;
	}

	public String getUserName() {
		return aUserName;
	}

	public void setUserName(String pUserName) {
		aUserName = pUserName;
	}

	public String getPassword() {
		return aPassword;
	}

	public void setPassword(String pPassword) {
		aPassword = pPassword;
	}

	public String getIPAddress() {
		return aIPAddress;
	}

	public void setIPAddress(String pIPAddress) {
		aIPAddress = pIPAddress;
	}

	public boolean isOnline() {
		return bOnline;
	}

	public void setOnline(boolean pOnline) {
		bOnline = pOnline;
	}
}